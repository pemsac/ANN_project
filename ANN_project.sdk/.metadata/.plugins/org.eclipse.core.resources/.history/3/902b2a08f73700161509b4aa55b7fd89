/*
 * Empty C++ Application
 */

#include <iostream>
#include <fstream>
#include "ff.h"
#include <string>

using namespace std;

int main()
{
  ifstream file;
  FATFS fileSystem;
  FRESULT fStatus;
  string num1=0 , num2=0;

  fStatus = f_mount(&fileSystem, "0:/", 0);
  if (fStatus != FR_OK)
    {
      cerr<<"ERROR"<<endl;
      return 0;
    }

  file.open("test.txt");

  if (file.is_open())
    cout << "error" << endl;
  else
    {
      cout << "Hello world" << endl;
      file >> num1 >> num2;
      cout << num1 << num2 << endl;
    }

  return 0;
}
