/*
 * Empty C++ Application
 */

#include <iostream>
#include <fstream>

using namespace std;

int main()
{
  ifstream file;
  FATFS fileSystem;
  FRESULT fStatus;
  int num1=0 , num2=0;

  fStatus = f_mount(&fileSystem, "0:/", 0);
  if (fStatus != FR_OK)
    {
      cerr<<"ERROR: impossible to mount SD memory on "<<FS_PATH<<endl;
      XGpio_DiscreteWrite(&gpio,LED_CHANNEL,LED_ERROR_STATE);
      return XST_FAILURE;
    }

  file.open("test.txt");

  if (file.is_open())
    cout << "error" << endl;
  else
    {
      cout << "Hello world" << endl;
      file >> num1 >> num2;
      cout << num1 << num2 << endl;
    }

  return 0;
}
